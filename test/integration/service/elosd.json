{
    "root": {
        "elos": {
            "UseEnv": true,
            "LogFilter": "",
            "LogLevel": "DEBUG",
            "ClientInputs": {
                "Plugins": {
                    "LocalTcpClient": {
                        "File": "client_tcp.so",
                        "Run": "always",
                        "Config": {
                            "ConnectionLimit": 200,
                            "Port": 54326,
                            "Interface": "127.0.0.1",
                            "EventBlacklist": ".event.messageCode 1000 LE",
                            "authorizedProcesses": [
                                "1 1 EQ"
                            ]
                        }
                    }
                }
            },
            "EventLogging": {
                "Plugins": {
                    "fetchapi": {
                        "File": "backend_fetchapi.so",
                        "Run": "always",
                        "Filter": [
                            "1 1 EQ"
                        ],
                        "Config": {
                            "BufferSize": 100
                        }
                    },
                    "JsonBackend": {
                        "File": "backend_json.so",
                        "Run": "always",
                        "Filter": [
                            "1 1 EQ"
                        ],
                        "Config": {
                            "StoragePath": "/tmp/elosd_%host%_%date%_%count%.log",
                            "MaxSize": 60000,
                            "Flags": [
                                "O_SYNC"
                            ]
                        }
                    }
                }
            },
            "Scanner": {
                "Plugins": {
                    "ScannerDummy": {
                        "File": "scanner_dummy.so",
                        "Run": "always"
                    }
                }
            }
        }
    }
}
